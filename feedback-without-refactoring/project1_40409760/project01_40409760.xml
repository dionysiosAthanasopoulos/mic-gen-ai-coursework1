<?xml version="1.0" encoding="UTF-8"?>
<sys>
    <name>decomposition</name>
    <!-- microservice 1 -->
    <ser>
        <name>mic1</name>
        <pack>
            <name>mic1.part1</name>
            <c>
                <name>mic1.part1.Controller</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <met>
                    <name>mic1.part1.Controller.Controller</name>
                    <access>PUBLIC</access>
                    <par>
                        <name>mic1.part1.Controller.Controller.role</name>
                        <cName>mic2.part1.Role</cName>
                    </par>
                    <local>
                        <name>mic1.part1.Controller.Controller.architects</name>
                        <cName>mic3.part2.Architects</cName>
                    </local>
                    <local>
                        <name>mic1.part1.Controller.Controller.engineers</name>
                        <cName>mic4.part3.Engineers</cName>
                    </local>
                    <ret>
                        <cName>mic1.part1.Controller</cName>
                    </ret>
                </met>
                <uRel>
                    <cName>mic2.part1.Role</cName>
                    <type>ASSOCIATION</type>
                </uRel>
                <uRel>
                    <cName>mic3.part2.Architects</cName>
                    <type>DEPENDENCY</type>
                </uRel>
                <uRel>
                    <cName>mic4.part3.Engineers</cName>
                    <type>DEPENDENCY</type>
                </uRel>
            </c>
        </pack>
    </ser>
    <!-- microservice 2 -->
    <ser>
        <name>mic2</name>
        <pack>
            <name>mic2.part1</name>
            <c>
                <name>mic2.part1.Role</name>
                <access>PUBLIC</access>
                <type>ENUM</type>
            </c>
        </pack>
    </ser>
    <!-- microservice 3 -->
    <ser>
        <name>mic3</name>
        <pack>
            <name>mic3.part2</name>
            <c>
                <name>mic3.part2.Architect</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <attr>
                    <name>mic3.part2.Architect.projects</name>
                    <access>PRIVATE</access>
                    <cName>mic3.part4.Project</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic3.part2.Architect</inName>
                </attr>
                <attr>
                    <name>mic3.part2.Architect.projectNumbers</name>
                    <access>PRIVATE</access>
                    <cName>mic4.part4.ProjectNumber</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic3.part2.Architect</inName>
                </attr>
                <uRel>
                    <cName>mic3.part2.Architects</cName>
                    <type>COMPOSITION</type>
                </uRel>
            </c>
            <c>
                <name>mic3.part2.Architects</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <attr>
                    <name>mic3.part2.Architects.architects</name>
                    <access>PRIVATE</access>
                    <cName>mic3.part2.Architect</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic3.part2.Architects</inName>
                </attr>
            </c>
        </pack>
        <pack>
            <name>mic3.part4</name>
            <c>
                <name>mic3.part4.Project</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <attr>
                    <name>mic3.part4.Project.projectNumber</name>
                    <access>PRIVATE</access>
                    <cName>mic4.part4.ProjectNumber</cName>
                    <inName>mic3.part4.Project</inName>
                </attr>
                <uRel>
                    <cName>mic3.part2.Architect</cName>
                    <type>COMPOSITION</type>
                </uRel>
                <uRel>
                    <cName>mic4.part3.Engineer</cName>
                    <type>COMPOSITION</type>
                </uRel>
            </c>
        </pack>
    </ser>
    <!-- microservice 4 -->
    <ser>
        <name>mic4</name>
        <pack>
            <name>mic4.part3</name>
            <c>
                <name>mic4.part3.Engineer</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <attr>
                    <name>mic4.part3.Engineer.projects</name>
                    <access>PRIVATE</access>
                    <cName>mic3.part4.Project</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic4.part3.Engineer</inName>
                </attr>
                <attr>
                    <name>mic4.part3.Engineer.projectNumbers</name>
                    <access>PRIVATE</access>
                    <cName>mic4.part4.ProjectNumber</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic4.part3.Engineer</inName>
                </attr>
                <uRel>
                    <cName>mic4.part3.Engineers</cName>
                    <type>COMPOSITION</type>
                </uRel>
            </c>
            <c>
                <name>mic4.part3.Engineers</name>
                <access>PUBLIC</access>
                <type>CLASS</type>
                <attr>
                    <name>mic4.part3.Engineers.engineers</name>
                    <access>PRIVATE</access>
                    <cName>mic4.part3.Engineer</cName>
                    <dName>java.util.ArrayList</dName>
                    <inName>mic4.part3.Engineers</inName>
                </attr>
            </c>
        </pack>
        <pack>
            <name>mic4.part4</name>
            <c>
                <name>mic4.part4.ProjectNumber</name>
                <access>PUBLIC</access>
                <type>ENUM</type>
                <uRel>
                    <cName>mic3.part2.Architect</cName>
                    <type>COMPOSITION</type>
                </uRel>
                <uRel>
                    <cName>mic4.part3.Engineer</cName>
                    <type>COMPOSITION</type>
                </uRel>
                <uRel>
                    <cName>mic3.part4.Project</cName>
                    <type>COMPOSITION</type>
                </uRel>
            </c>
        </pack>
    </ser>
</sys>