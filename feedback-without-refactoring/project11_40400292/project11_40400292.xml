<?xml version="1.0" encoding="UTF-8"?>
<sys>
	<name>decomposition</name>
		
		<!-- first microservice -->
		<ser>
			<name>mic1</name>
			<pack>
				<name>mic1.part1</name>
				<c>
					<name>mic1.part1.Role</name>
					<access>PUBLIC</access>
					<type>ENUM</type>
				</c>
			</pack>
		</ser>
		
		<!-- second microservice -->
		<ser>
			<name>mic2</name>
        	<pack>
            	<name>mic2.part1</name>
            	<c>
                	<name>mic2.part1.Controller</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>
                	<met>
                		<name>mic2.part1.Controller.Controller</name>
                		<access>PUBLIC</access>
                		<par>
                			<name>mic2.part1.Controller.Controller.role</name>
                			<cName>part1.Role</cName>
                		</par>
                	</met>
                	<uRel>
						<cName>mic1.part1.Role</cName>
						<type>ASSOCIATION</type>
					</uRel>
					<uRel>
						<cName>mic81.part3.Engineers</cName>
						<type>DEPENDENCY</type>
					</uRel>
					<uRel>
						<cName>mic71.part2.Architects</cName>
						<type>DEPENDENCY</type>
					</uRel>
            	</c>
        	</pack>
		</ser>
		
		<!-- third microservice -->
		<ser>
			<name>mic71</name>
			
			<pack>
            	<name>mic71.part2</name>
            	
            	<c>
                	<name>mic71.part2.Architects</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>
                	<attr>
                		<name>mic71.part2.Architects.architects</name>
                		<cName>mic71.part2.Architect</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic71.part2.Architects</inName>
                	</attr>
            	</c>
            	
            	<c>
                	<name>mic71.part2.Architect</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>  
                	<uRel>
                		<cName>mic71.part2.Architects</cName>
                		<type>COMPOSITION</type>
                	</uRel> 
                	                             	
                	<attr>
                    	<name>mic71.part2.Architect.projects</name>
                    	<cName>mic71.part4.projects</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic71.part2.Architects</inName>
               		</attr>
               		
                	<attr>
                		<name>mic71.part2.Architect.projectNumbers</name>
                    	<cName>mic81.part4.projectNumber</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic71.part2.Architects</inName>                  	                    	
                	</attr>
            	</c>
        	</pack>
        
        	<pack>
            	<name>mic71.part4</name>
            	<c>
                	<name>mic71.part4.Project</name>
                	<type>CLASS</type>
                	<access>PUBLIC</access>
                	<attr>
                		<name>mic71.part4.Project.projectNumber</name>
                    	<cName>mic81.part4.projectNumber</cName>              		
                		<access>PRIVATE</access>
                		<inName>mic71.part4.Project</inName>                  	                    	
                	</attr>
                	<uRel>
						<cName>mic81.part3.Engineer</cName>
						<type>COMPOSITION</type>
					</uRel>  
					<uRel>
						<cName>mic71.part2.Architect</cName>
						<type>COMPOSITION</type>
					</uRel>               	
            	</c>
        	</pack>	
		</ser>
		
		<!-- fourth microservice -->
		<ser>
			<name>mic81</name>
			<pack>
            	<name>mic81.part3</name>
            	
            	<c>
                	<name>mic81.part3.Engineer</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>
                	<attr>
                		<name>mic81.part3.Engineer.projects</name>
                    	<cName>mic71.part4.Project</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic81.part3.Engineer</inName> 
                	</attr>
                	<attr>
                		<name>mic81.part3.Engineer.projectNumbers</name>
                    	<cName>mic81.part4.ProjectNumber</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic81.part3.Engineer</inName> 
                	</attr>
                	<uRel>
                		<cName>mic81.part3.Engineers</cName>
						<type>COMPOSITION</type>
                	</uRel>
            	</c>
            	<c>
                	<name>mic81.part3.Engineers</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>
                	<attr>
                		<name>mic81.part3.Engineers.engineers</name>
                    	<cName>mic81.part3.Engineer</cName>
                		<dName>java.util.ArrayList</dName>
                		<access>PRIVATE</access>
                		<inName>mic81.part3.Engineers</inName> 
                	</attr>
                	 
            	</c>
           </pack>
           
           <pack>
           		<name>mic81.part4</name>
            	<c>
                	<name>mic81.part4.ProjectNumber</name>
                	<access>PUBLIC</access>
                	<type>CLASS</type>
                	<uRel>
                		<cName>mic71.part2.Architect</cName>
						<type>COMPOSITION</type>
                	</uRel>
                	<uRel>
                		<cName>mic71.part4.Project</cName>
						<type>COMPOSITION</type>
                	</uRel>
                	<uRel>
                		<cName>mic81.part3.Engineer</cName>
						<type>COMPOSITION</type>
                	</uRel>
            	</c>
            </pack>
		</ser>
</sys>